
@{
    ViewBag.Title = "Seguimiento";
    Layout = "~/Views/Shared/LayoutUsuario.cshtml";
}

<div class="content">
    <div class="container">

        <div class="row m-t-40">
            <div class="col-sm-12">
                <div class="text-center">
                    <h2 class="text-success m-t-0 text-uppercase font-600">Proximamente</h2>
                </div>
            </div>
        </div>

        <div class="row">

            <div class="col-md-12">
                <div id="count-down" class="row center-block">
                    <div class="clock-presenter days_dash col-sm-3">
                        <div class="digit"></div>
                        <div class="digit"></div>
                        <div class="digit"></div>
                        <p class='note dash_title'>Dias</p>
                    </div>
                    <div class="clock-presenter hours_dash col-sm-3">
                        <div class="digit"></div>
                        <div class="digit"></div>
                        <p class='note dash_title'>Horas</p>
                    </div>
                    <div class="clock-presenter minutes_dash col-sm-3">
                        <div class="digit"></div>
                        <div class="digit"></div>
                        <p class='note dash_title'>Minutos</p>
                    </div>
                    <div class="clock-presenter seconds_dash col-sm-3">
                        <div class="digit"></div>
                        <div class="digit"></div>
                        <p class='note dash_title'>Segundos</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

<!--Efecto de seleccion-->
<script src="~/Source/js/Indices.js"></script>

<!--Lectura imagenes de perfil-->
<script src="~/Source/js/ImagenDePerfil.js"></script>

<!--Jquery-->
<script src="~/Source/User/js/jquery.min.js"></script>

<!--Efecto de conteo-->
<script type="text/javascript">
    $(document).ready(function () {

        "use strict";

        //Set your date
        $('#count-down').countDown({
            targetDate: {
                'day': 19,
                'month': 5,
                'year': 2021,
                'hour': 20,
                'min': 50,
                'sec': 0
            },
            omitWeeks: true
        });

    });
</script>